<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_range_seekbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Range seekbar"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_video_player"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Video Player"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_video_timeline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Video Timeline"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_svg_path_loader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Load SVG"
        android:textAllCaps="false" />


</LinearLayout>